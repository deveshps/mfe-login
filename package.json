{
  "name": "mfe-remote1",
  "version": "0.1.0",
  "description": "Microfrontend 1",
  "main": "index.js",
  "scripts": {
    "dev": "DEV_SERVER=true webpack-dev-server --hot --port 3001 --config config/webpack.client.js",
    "build": "npm run clean && npm run build:client",
    "build:client": "webpack --progress --config config/webpack.client.js",
    "start": "npm run build && serve -s dist -p 3001",
    "clean": "rm -rf dist",
    "blame": "git log -S",
    "hist": "git log --oneline | grep -i",
    "follow": "git log --follow --oneline"
  },
  "author": "Abhishek Raj <abhishekr@dewsolutions.in>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.1",
    "serve": "^14.2.0",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
